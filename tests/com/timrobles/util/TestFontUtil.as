package com.timrobles.util {    import flexunit.framework.TestCase;    import flash.text.Font;    /**     * @author timrobles     */    public class TestFontUtil extends TestCase     {        //--------------------------------------------------------------------------        //        //  Tests        //        //--------------------------------------------------------------------------        [Test]        public function testHasFont():void         {             var fonts:Array = Font.enumerateFonts(true);            for each (var font : Font in fonts)            {                assertTrue(FontUtil.hasFont(font.fontName), "Expected has font to return true for " + font.fontName);            }	        }    }}