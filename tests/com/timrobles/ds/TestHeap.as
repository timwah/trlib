package com.timrobles.ds {	import flexunit.framework.TestCase;	import flexunit.framework.TestSuite;		/**	 * @author timrobles	 */	public class TestHeap extends TestCase {				//--------------------------------------------------------------------------		//		//  Constructor		//		//--------------------------------------------------------------------------				public function TestHeap( methodName : String = null ) {						super( methodName );					}				public static function suite() : TestSuite {						var ts : TestSuite = new TestSuite();			ts.addTest( new TestHeap( "testContains" ) );			ts.addTest( new TestHeap( "testIsEmpty" ) );			return ts;						}				public function testContains() : void {					var testHeap : Heap = new Heap( false, 1, 2, 3 );			assertTrue( "Expected heap to contain 3, instead returned false.", testHeap.contains( 3 ) );						}				public function testIsEmpty() : void {					var emptyHeap : Heap = new Heap();			assertTrue( "Expected heap to be empty.", emptyHeap.isEmpty() );						}			}	}