package com.timrobles.ds {    import org.flexunit.asserts.assertTrue;    /**     * @author timrobles     */    public class TestHeap     {    	//--------------------------------------------------------------------------    	//    	//  Test Cases    	//    	//--------------------------------------------------------------------------    			[Test]        public function testContains():void         {            var testHeap:Heap = new Heap(false, 1, 2, 3);            assertTrue("Expected heap to contain 3, instead returned false.", testHeap.contains(3));        }				[Test]        public function testIsEmpty():void         {            var emptyHeap:Heap = new Heap();            assertTrue("Expected heap to be empty.", emptyHeap.isEmpty());        }    }}