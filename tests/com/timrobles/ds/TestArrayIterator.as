package com.timrobles.ds {    import org.flexunit.asserts.assertFalse;    import org.flexunit.asserts.assertTrue;    /**     * @author timrobles     */    public class TestArrayIterator    {    	        //--------------------------------------------------------------------------        //        //  Test Cases        //        //--------------------------------------------------------------------------		[Test]        public function testNext():void         {    		            var values:Array = new Array(3, 2, 1);            var arrIterator:Iterator = new ArrayIterator(values);            var n:uint = values.length;            for (var i:uint = 0; i < n; i++)             {                var value:uint = arrIterator.next();                assertTrue("Expected to get " + values[ i ] + ", got " + value + " instead.", value == values[ i ]);            }    		            var hasError:Boolean = false;            var nextValue:* = null;            try             {                nextValue = arrIterator.next();               }             catch (err:Error)             {                hasError = true;            }            assertTrue("Expected to exceed bounds and throw error, instead got: " + nextValue, hasError);        }		[Test]        public function testHasNext():void         {            var values:Array = new Array(1, 1);            var arrIterator:Iterator = new ArrayIterator(values);            var n:uint = values.length;            for (var i:uint = 0; i < n; i++)             {                assertTrue("Expected hasNext() to be true, instead got: " + arrIterator.hasNext(), arrIterator.hasNext());                arrIterator.next();            }            assertFalse("Expected hasNext() to be false, instead got: " + arrIterator.hasNext(), arrIterator.hasNext());        }    }}