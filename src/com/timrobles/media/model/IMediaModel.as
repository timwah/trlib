package com.timrobles.media.model {    import flash.events.IEventDispatcher;                                /**     * @author Tim Robles     */    public interface IMediaModel extends IEventDispatcher    {    	function load(url:String, autoPlay:Boolean = true):void;    	function play():void;    	function pause():void;    	function togglePause():void;    	function stop():void;    	function close():void;		function getMetadata():Object;        function getBytesLoaded():uint;        function getBytesTotal():uint;            	    	// position returns a percentage, or total complete    	function set position(value:Number):void;        function get position():Number;		// time returns the actual playhead position in milliseconds        function set time(value:Number):void;        function get time():Number;        function set volume(value:Number):void;        function get volume():Number;        function set panning(value:Number):void;        function get panning():Number;                function set bufferTime(value:Number):void;    	function get bufferTime():Number;		function set url(value:String):void;    	function get url():String;    	    	function set playing(value:Boolean):void;		function get playing():Boolean;				function set scrubbing(value:Boolean):void;		function get scrubbing():Boolean;				function get initialized():Boolean;    }}