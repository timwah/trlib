package com.timrobles.ds {
    /**     * @author timrobles     */    public class ArrayIterator implements Iterator {    	    	//----------------------------------    	//  Protected Variables    	//----------------------------------    	    	protected var cursor : uint = 0;    	protected var values : Array;    	    	//--------------------------------------------------------------------------    	//    	//  Constructor    	//    	//--------------------------------------------------------------------------    	    	public function ArrayIterator( values : Array ) {    		    		cursor = 0;            this.values = values;            	        }                //--------------------------------------------------------------------------        //        //  Public Methods        //        //--------------------------------------------------------------------------                public function hasNext() : Boolean {        	        	return cursor < values.length;        	        }                public function next() : * {        	        	if ( cursor >= values.length )                throw new Error( "Cannot iterate further than values[ " + cursor + " ]." );                        return values[ cursor++ ];                    }            }    }